{
  "manifest_version": 2,
  "short_name": "Easy Mail",
  "name": "Easy Mail",
  "description": "Easy Mail: Get youe test mail easily.",
  "version": "0.1",
  "browser_action": {
    "default_popup": "index.html"
  },
  "web_accessible_resources": ["js/*", "html/*", "css/*"],
  "permissions": ["activeTab"],
  "content_security_policy": "script-src 'self' 'sha256-PNqGbvK5opZR3o9fRWXdbL9pgEbToI+IO+nOgDBp76o='; object-src 'self'",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "Akas Rai"
}
